# Most of this should be exported.
--svn-sync-process-export all
/tests/api/tdOci*|/tests/api/oci*|/tests/api/tdCloud*:   --svn-sync-process-export none
## @todo export: Why is the docker stuff kept private?
/testmanager/docker*:                   --svn-sync-process-export none
## @todo export: Why is the usbtest.service kept private?
/utils/usb/linux/usbtest.service:       --svn-sync-process-export none

# The validation kit is dual licensed.
--license-ose-dual

*.c16:                                  --treat-as .c
*.c32:                                  --treat-as .c
*.c64:                                  --treat-as .c

*.css:                                  --treat-as .h --no-fix-header-guards
*.js:                                   --treat-as .h --no-fix-header-guards

/testdriver/*.ps1:                      --treat-as Makefile
/testmanager/apache-template-*.conf:    --treat-as Makefile
/testmanager/misc/htpasswd-*:           --treat-as plaintext

# No (C) or license in the html templates.
/testmanager/webui/*.html:              --external-copyright # generated from the .txt files

# Some stuff without (easily locatable) licenses and such.
/docs/*.html:                           --external-copyright # generated from the .txt files

# XML for database diagrams.
/testmanager/db/TestManagerDatabase.dmd:    --3rd-party --treat-as xml
/testmanager/db/TestManagerDatabase/*.xml:  --3rd-party

# Test data.
/tests/*.nvram:                         --treat-as binary
/tests/*.pem:                           --treat-as plaintext --no-strip-trailing-blanks
/tests/*.ova:                           --treat-as binary
/tests/*.vdi:                           --treat-as binary
/utils/usb/linux/usbtest.service:       --treat-as plaintext --del-action force-native-eol --add-action force-lf

# TXS configure files.
/utils/TestExecServ/linux/*.desktop:    --treat-as plaintext --del-action force-native-eol --add-action force-lf
/utils/TestExecServ/linux/*.service:    --treat-as plaintext --del-action force-native-eol --add-action force-lf
/utils/TestExecServ/win/*.reg:          --treat-as plaintext --del-action force-native-eol --add-action force-crlf

# misc
/bootsectors/bs3kit/bs3kit-mangling-code.h:        --no-fix-header-guards
/bootsectors/bs3kit/bs3kit-mangling-code-define.h: --no-fix-header-guards
/bootsectors/bs3kit/bs3kit-mangling-code-undef.h:  --no-fix-header-guards
/bootsectors/bs3kit/bs3kit-template-header.h:      --no-fix-header-guards
/bootsectors/bs3kit/bs3kit-template-footer.h:      --no-fix-header-guards
/bootsectors/bs3kit/bs3-cmn-instantiate-common.h:  --no-fix-header-guards
/bootsectors/bs3kit/*.h: --guard-relative-to-dir bootsectors/bs3kit/ --guard-prefix BS3KIT_INCLUDED_

